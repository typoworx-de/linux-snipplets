#!/bin/bash

browser=google-chrome
date=$(date '+%Y%m%d-%H%I')

tar cvjf $(pwd)/${browser}~${date}.tar.bz2 ~/.config/${browser} \
 --exclude=LOG \
 --exclude=LOG.* \
 --exclude=LOCK \
 --exclude=*.tmp \
 --exclude=*/*downloads*/* \
 --exclude=*/*Cache*/* \
 --exclude=*/*cache*/* \
 --exclude=ServiceWorker/* \
 --exclude=GrShaderCache/* \
 --exclude=GPUCache/* \
;
